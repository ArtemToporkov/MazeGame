class Game {

    function void start() {

        var Level firstLevel;
        var Level secondLevel;

        let firstLevel = Level.new("Level 1: The IRIT-RTF Redemption", 1);
        do firstLevel.run();

        let secondLevel = Level.new("Level 2: Unexpected Turns", 2);
        do secondLevel.run();

        do Output.moveCursor(11, 22);
        do Output.printString("CONGRATULATIONS!!!");

        return;
    }
}