class Wall {
    field Point begin, end;

    constructor Wall new(Point beginLocation, Point endLocation) {
        let begin = beginLocation;
        let end = endLocation;
        return this;
    }

    method void dispose() {
        do location.dispose();
        do Memory.deAlloc(this);

        return;
    }

    method void draw() {
        do Screen.setColor(true);
        do Screen.drawRectangle(begin.X(), begin.Y(), end.X(), end.Y());

        return;
    }
}